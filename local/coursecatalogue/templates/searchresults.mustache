<div class="card">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Course Name</th>
        <th>Course ID</th>
        <th>Start Date</th>
        <th>Tags</th>
        <th>Instructor</th>
        <th>Ratings</th>
      </tr>
    </thead>
    {{#data}}
      <tr>
        <td>{{{id}}}</td>
        <td>{{{name}}}</td>
        <td>{{{shortname}}}</td>
        <td>{{description}}</td>
        <td>${{sortorder}}</td>
      <td style="width:20px;"><a href="/local/moodle/graderdetail.php?grader={{id}}&month={{month}}&year={{year}}"><i class="fa fa-arrow-circle-right fa-2x"></i></td>
      </tr>
    {{/data}}
  </table>
  {{^data}}
  <div class="card-body"><p>There are no results</p></div>
    {{/data}}
</div>
